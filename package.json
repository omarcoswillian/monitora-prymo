{
  "name": "monitor-pages",
  "version": "1.0.0",
  "description": "Monitor web pages for changes and availability",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev:monitor": "tsx watch src/index.ts",
    "dev:dash": "npm --prefix dashboard run dev",
    "dev": "concurrently \"npm run dev:monitor\" \"npm run dev:dash\"",
    "start": "node dist/index.js",
    "lint": "eslint src/",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "monitor",
    "web",
    "pages",
    "availability",
    "uptime"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "concurrently": "^8.2.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.91.1",
    "dotenv": "^17.2.3",
    "node-cron": "^4.2.1"
  }
}
